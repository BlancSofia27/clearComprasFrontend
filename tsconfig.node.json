{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react"  // Asegúrate de que esta opción esté configurada
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
